// assignment # 1 //
void main() {
  if (isPalindrome("zoo")) {
    print("Palindrome");
  } else {
    print("Not Palindrome");
  }
}

bool isPalindrome(String input) {
  for (int a = 0; a < input.length; a++) {
    if (input[a] != input[input.length - 1 - a]) {
      return false;
    }
  }
  return true;
}
